{
  "name": "@team-plain/docs-mintlify",
  "version": "1.0.0",
  "description": "Documentation for plain.com",
  "main": "index.js",
  "engines": {
    "node": "18.x",
    "pnpm": "8.x"
  },
  "scripts": {
    "dev": "mintlify dev",
    "lint": "pnpm run lint:links && pnpm run lint:prettier",
    "lint:links": "mintlify broken-links",
    "lint:prettier": "prettier --check './**/*.{js,ts,tsx,md,mdx}'"
  },
  "dependencies": {
    "mintlify": "^4.0.58",
    "prettier": "^3.1.0"
  }
}
